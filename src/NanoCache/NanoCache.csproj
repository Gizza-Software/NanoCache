<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
		<LangVersion>latest</LangVersion>
	</PropertyGroup>

	<PropertyGroup>
		<PackageId>NanoCache</PackageId>
		<Authors>Burak Ã–ner</Authors>
		<Version>1.0.0</Version>
		<FileVersion>1.0.0</FileVersion>
		<PackageVersion>1.0.0</PackageVersion>
		<AssemblyVersion>1.0.0</AssemblyVersion>
		<Description>Faster Distributed Cache Solution, including Cache Server and Client with dependency injections</Description>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
		<PackageTags>NanoCode;NanoCache;Cache;Server;Client;TCP;Socket;Dependency Injection</PackageTags>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<NeutralLanguage>en</NeutralLanguage>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageReleaseNotes>
			Version 1.0.0 - 15 Oct 2022
		</PackageReleaseNotes>
		<PackageIconUrl />
		<PackageProjectUrl>https://github.com/burakoner/NanoCache</PackageProjectUrl>
		<RepositoryUrl>https://github.com/burakoner/NanoCache</RepositoryUrl>
		<PackageIcon>NanoCache.png</PackageIcon>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="MessagePack" Version="2.4.35" />
		<PackageReference Include="Microsoft.Extensions.Caching.Abstractions" Version="6.0.0" />
		<PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="6.0.0" />
		<PackageReference Include="Microsoft.Extensions.Hosting.Abstractions" Version="6.0.0" />
		<PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="6.0.2" />
		<PackageReference Include="Microsoft.Extensions.Options" Version="6.0.0" />
		<PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
		<PackageReference Include="TcpSharp" Version="1.0.1" />
	</ItemGroup>

	<ItemGroup>
	  <None Include="NanoCache.png" Pack="True" PackagePath="" />
	</ItemGroup>

</Project>
